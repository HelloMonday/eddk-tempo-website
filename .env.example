# Contentful CMS Configuration
# Get these values from Contentful: Settings > API keys

# Your Contentful Space ID
CONTENTFUL_SPACE_ID=your_space_id_here

# Content Delivery API - Access Token (for published content)
CONTENTFUL_ACCESS_TOKEN=your_access_token_here

# Content Preview API - Access Token (optional, for draft content)
CONTENTFUL_PREVIEW_TOKEN=your_preview_token_here

# Environment (usually 'master')
CONTENTFUL_ENVIRONMENT=master

# Content Management API Token (for migrations and content seeding)
CONTENTFUL_CMA_TOKEN=your_cma_token_here
